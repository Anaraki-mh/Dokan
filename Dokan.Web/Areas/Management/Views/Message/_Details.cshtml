@model Dokan.Web.Areas.Management.Models.MessageModel

@{
    ViewBag.Title = "_Details";
}

<div class="">
    <div class="card-body">
        <h4 class="card-title">Details</h4>
        <p class="card-description">

        </p>
        <form class="forms-sample">

            <div class="form-group">
                @Html.LabelFor(x => x.Name)
                @Html.TextBoxFor(x => x.Name, new { @class = "form-control", @readonly = true })
            </div>

            <div class="form-group">
                @Html.LabelFor(x => x.Email)
                @Html.TextBoxFor(x => x.Email, new { @class = "form-control", @readonly = true })
            </div>

            <div class="form-group">
                @Html.LabelFor(x => x.Subject)
                @Html.TextBoxFor(x => x.Subject, new { @class = "form-control", @readonly = true })
            </div>

            <div class="form-group">
                @Html.LabelFor(x => x.MessageBody)
                @Html.TextBoxFor(x => x.MessageBody, new { @class = "form-control", @readonly = true })
            </div>

        </form>
    </div>
</div>
