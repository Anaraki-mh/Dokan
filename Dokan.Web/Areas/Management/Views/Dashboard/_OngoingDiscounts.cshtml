@model List<Dokan.Domain.Website.DiscountCategory>

@if (Model.Count > 0)
{


    <table class="table table-responsive-sm table-hover text-center">
        <thead>
            <tr>
                <th>Title</th>
                <th>Discount</th>
                <th>Expiry Date</th>

            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Title</td>
                    <td>@item.Discount</td>
                    <td>@item.ExpiryDateTime</td>
                </tr>
            }

        </tbody>
    </table>


}