@model Dokan.Web.Areas.Management.Models.TestimonialModel

@{
    ViewBag.Title = "_Details";
}

<div class="">
    <div class="card-body">
        <h4 class="card-title">Details</h4>
        <p class="card-description">
            
        </p>
        <form class="forms-sample">

            <div class="form-group">
                @Html.LabelFor(x => x.Image)
                @Html.TextBoxFor(x => x.Image, new { @class = "form-control", @readonly = true })
                <img src="~/Files/@Model.Image" class="" />
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.FullName)
                @Html.TextBoxFor(x => x.FullName, new { @class = "form-control", @readonly = true })
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Position)
                @Html.TextBoxFor(x => x.Position, new { @class = "form-control", @readonly = true })
            </div>
            <div class="form-group">
                @Html.LabelFor(x => x.Content)
                @Html.TextBoxFor(x => x.Content, new { @class = "form-control", @readonly = true })
            </div>

        </form>
    </div>
</div>
